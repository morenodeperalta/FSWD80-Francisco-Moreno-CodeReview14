<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Eventable" table="eventable">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <field name="description" type="string" column="description" length="4000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="img" type="string" column="img" length="4000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="capacity" type="string" column="capacity" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="300" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="website" type="string" column="website" length="300" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
